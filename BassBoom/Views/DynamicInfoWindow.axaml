<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        SizeToContent="WidthAndHeight"
        x:Class="BassBoom.Views.DynamicInfoWindow"
        Title="Info window">
    <StackPanel>
        <Grid Name="DynamicGrid" RowDefinitions="Auto, Auto" ColumnDefinitions="Auto, Auto" Margin="15">
        </Grid>
        <Grid RowDefinitions="Auto" ColumnDefinitions="Auto" Margin="15" HorizontalAlignment="Right">
            <Button Grid.Row="1" Grid.Column="0" Name="Acknowledge" x:CompileBindings="False" Command="{Binding Acknowledge}">OK</Button>
        </Grid>
    </StackPanel>
</Window>
